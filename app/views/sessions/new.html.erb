<div class="sign_up">
<h2>Login</h2>

<%= form_for :session, url: sessions_path, :html => { :class => "pure-form pure-form-stacked"} do |f| %>

  <%= f.label :email %>
  <%= f.text_field :email%>

  <%= f.label :password %>
  <%= f.password_field :password%>

  <%= f.submit "Submit", :class => 'pure-button' %>
  
<% end %>
</div>
