<h2>Sign Up</h2>
<% if @errors %>
<ul>
  <% @errors.each do |e| %>
  <li><%= e %></li>
  <% end %>
</ul>
<% end %>
<%= form_for @user do |f| %>
  <%= f.label :first_name %>:
  <%= f.text_field :first_name %><br />

  <%= f.label :last_name %>:
  <%= f.text_field :last_name %><br />

  <%= f.label :email %>:
  <%= f.text_field :email%><br />

  <%= f.label :password %>:
  <%= f.password_field :password%><br />

  <%= f.label :phone_number %>:
  <%= f.text_field :phone_number%><br />

  <%= f.submit %>
<% end %>
